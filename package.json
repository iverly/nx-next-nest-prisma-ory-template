{
  "name": "@nx-next-nest-prisma-ory-template/source",
  "version": "0.0.0",
  "scripts": {
    "build": "nx run-many --target=build --all=true",
    "dev:api": "nx run api:serve:development --watch=true",
    "dev:docker": "docker compose -f docker-compose.base.yaml -f docker-compose.dev.yaml",
    "dev:docker:up": "pnpm dev:docker up -d",
    "dev:docker:down": "pnpm dev:docker down",
    "prisma:generate": "rimraf packages/types/src/lib/generated && prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "rimraf packages/types/src/lib/generated && prisma migrate reset --force",
    "db:push": "prisma db push"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/platform-fastify": "^10.2.10",
    "@prisma/client": "5.6.0",
    "axios": "^1.0.0",
    "fastify": "^4.24.3",
    "joi": "^17.11.0",
    "prisma": "^5.6.0",
    "prisma-generator-nestjs-dto": "^1.1.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/eslint": "17.1.2",
    "@nx/eslint-plugin": "17.1.2",
    "@nx/jest": "17.1.2",
    "@nx/js": "17.1.2",
    "@nx/nest": "17.1.2",
    "@nx/node": "17.1.2",
    "@nx/webpack": "17.1.2",
    "@nx/workspace": "17.1.2",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "~8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "nx": "17.1.2",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.2"
  },
  "private": true,
  "license": "Apache-2.0"
}
